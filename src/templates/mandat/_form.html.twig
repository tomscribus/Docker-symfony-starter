<div class="user container">
{{ form_start(form) }}
  <div class="form-group">
    {{ form_row(form.document, { 'attr': {'class': 'form-control form-control-user'} } ) }}
    {{ form_row(form.client, { 'attr': {'class': 'form-control form-control-user'} } ) }}
    {{ form_row(form.broker, { 'attr': {'class': 'form-control form-control-user'} } ) }}
  </div>
    <button type="submit" class="btn btn-primary btn-user btn-block">{{ button_label|default('Enregistrer') }}</button>
{{ form_end(form) }}
</div>
