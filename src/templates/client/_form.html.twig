
<div class="user container">
{{ form_start(form) }}
  <div class="form-group">
    {{ form_row(form.name, { 'attr': {'class': 'form-control form-control-user'} } ) }}
    {{ form_row(form.surname, { 'attr': {'class': 'form-control form-control-user'} } ) }}
    {{ form_row(form.address, { 'attr': {'class': 'form-control form-control-user'} } ) }}
    {{ form_row(form.email, { 'attr': {'class': 'form-control form-control-user'} } ) }}
  </div>
    <button type="submit" class="btn btn-primary btn-user btn-block">{{ button_label|default('Enregistrer') }}</button>
{{ form_end(form) }}
</div>
